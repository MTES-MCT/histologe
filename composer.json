{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "~8.4",
        "ext-sodium": "*",
        "beberlei/doctrineextensions": "^1.3",
        "codercat/jwk-to-pem": "^1.1",
        "composer/package-versions-deprecated": "1.11.99.4",
        "cron/cron": "^1.8",
        "doctrine/doctrine-bundle": "^3.2",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^3.6",
        "dompdf/dompdf": "^3.1",
        "endroid/qr-code": "^6.0",
        "getbrevo/brevo-php": "^1.0",
        "giggsey/libphonenumber-for-php": "^8.13",
        "intervention/image": "^2.7",
        "lcobucci/jwt": "^5.5",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-bundle": "^3.0",
        "maennchen/zipstream-php": "^3.2",
        "nelmio/api-doc-bundle": "^5",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpoffice/phpspreadsheet": "^2.2",
        "phpstan/phpdoc-parser": "^1.2",
        "scheb/2fa-bundle": "^8",
        "scheb/2fa-email": "^8",
        "scienta/doctrine-json-functions": "^5.0",
        "sentry/sentry-symfony": "^5.6",
        "sineflow/clamav": "^1.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "8.0.*",
        "symfony/brevo-mailer": "8.0.*",
        "symfony/cache": "8.0.*",
        "symfony/console": "8.0.*",
        "symfony/doctrine-messenger": "8.0.*",
        "symfony/dotenv": "8.0.*",
        "symfony/emoji": "8.0.*",
        "symfony/expression-language": "8.0.*",
        "symfony/flex": "^2",
        "symfony/form": "8.0.*",
        "symfony/framework-bundle": "8.0.*",
        "symfony/html-sanitizer": "8.0.*",
        "symfony/http-client": "8.0.*",
        "symfony/intl": "8.0.*",
        "symfony/lock": "8.0.*",
        "symfony/mailer": "8.0.*",
        "symfony/messenger": "8.0.*",
        "symfony/mime": "8.0.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "8.0.*",
        "symfony/process": "8.0.*",
        "symfony/property-access": "8.0.*",
        "symfony/property-info": "8.0.*",
        "symfony/rate-limiter": "^7.4",
        "symfony/runtime": "8.0.*",
        "symfony/security-bundle": "8.0.*",
        "symfony/serializer": "8.0.*",
        "symfony/string": "8.0.*",
        "symfony/translation": "8.0.*",
        "symfony/twig-bundle": "8.0.*",
        "symfony/uid": "8.0.*",
        "symfony/validator": "8.0.*",
        "symfony/web-link": "8.0.*",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/workflow": "8.0.*",
        "symfony/yaml": "8.0.*",
        "tilleuls/url-signer-bundle": "^2.2",
        "twig/cssinliner-extra": "^3.3",
        "twig/extra-bundle": "^3.3",
        "twig/inky-extra": "^3.3",
        "twig/intl-extra": "^3.3",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "8.4.*",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "ekino/phpstan-banned-code": "^2.1",
        "fakerphp/faker": "^1.20",
        "phpro/grumphp-shim": "^1.13",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^1.10.52",
        "phpstan/phpstan-doctrine": "^1.5",
        "phpstan/phpstan-symfony": "^1.4",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "8.0.*",
        "symfony/css-selector": "8.0.*",
        "symfony/debug-bundle": "8.0.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^8.0",
        "symfony/stopwatch": "8.0.*",
        "symfony/web-profiler-bundle": "8.0.*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true,
            "phpro/grumphp-shim": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cs-check": "tools/php-cs-fixer/vendor/bin/php-cs-fixer fix --dry-run -v",
        "cs-fix": "tools/php-cs-fixer/vendor/bin/php-cs-fixer fix",
        "stan": "vendor/bin/phpstan analyse --memory-limit 1G",
        "check-quality": [
            "@composer cs-check",
            "@composer stan"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "8.0.*"
        },
        "paas": {
            "compile": [
                "npm run build"
            ],
            "php-config": [
                "memory_limit=-1",
                "opcache.memory_consumption=256",
                "opcache.max_accelerated_files=20000",
                "opcache.validate_timestamps=0",
                "opcache.preload=config/preload.php",
                "opcache.preload_user=appsdeck",
                "realpath_cache_size=4096K",
                "realpath_cache_ttl=600"
            ],
            "nginx-includes": [
                ".scalingo/nginx/server.location"
            ]
        }
    }
}
