parameters:
	level: 6
	paths:
		- src
		- tests
	excludePaths:
		- .php-cs-fixer.dist.php
		- tools
		- tests/Functional/Command/NotifyVisitsCommandTest.php
		- src/DataFixtures/Loader/LoadInterventionData.php
	treatPhpDocTypesAsCertain: false
	checkGenericClassInNonGenericObjectType: false
	bootstrapFiles:
		- tools/wiremock/src/Mock/AppMock.php
	ignoreErrors:
		-
			message: '#Property [a-zA-Z0-9\\_]+::\$desordrePrecisionRepository is never read, only written.#'
			path: src/Service/Signalement/DesordreTraitement/*
		-
			message: '#Property .+::\$id is never written, only read.#'
			path: src/Entity/*
		-
			message: '#Should not use node with type "Stmt_Echo"#'
			path: src/DataFixtures/Loader/LoadInterventionData.php
		-
			message: '#Should not use node with type "Stmt_Echo"#'
			path: tests/Functional/Command/NotifyVisitsCommandTest.php
